{
    "version": 2,
    "builds": [
        {
            "src": "/src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/",
            "dest": "/server.js"
        }
    ]
}
