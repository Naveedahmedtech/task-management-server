{
    "version": 2,
    "builds": [
        {
            "src": "/server.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server.ts"
        },
        {
            "src": "/",
            "dest": "/server.ts"
        }
    ]
}


